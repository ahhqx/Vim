	<script type="text/javascript">
    (function(){
     
    var hasLocalStorage, trigger = document.getElementById('newSliderPage');

    // 检测localstorage
    try {
        hasLocalStorage = ('localStorage' in window) && window['localStorage']
    } catch(msg) {
        hasLocalStorage = false;
    };        
          
    if(hasLocalStorage){
        if(localStorage.getItem('startPage')) return;
        localStorage.setItem('startPage', '0');
        trigger.setAttribute('start', '0');
    } else if('\v' == 'v') {
        trigger.load('ieStartPage');
        if(trigger.getAttribute('gettingOld') == 'yes') {
            trigger.setAttribute('start','1'); 
            return;
        };
        trigger.setAttribute('gettingOld','yes');
        trigger.setAttribute('start','0');
        trigger.save('ieStartPage');
    };
        
    })();    
    </script>
