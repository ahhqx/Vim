function regExpMatch(url, pattern) {
	try { return new RegExp(pattern).test(url); } catch(ex) { return false; }
}

function FindProxyForURL(url, host) {
	if (regExpMatch(url, '.*://.*twitter\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
        if (regExpMatch(url, '.*://search\.twitter\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://d\.seesmic\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://www\.imageurlhost\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://tools\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://clients2\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://clients2\.googleusercontent\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*\.flickr\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://spreadsheets\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://goo\.gl/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://feeds\.feedburner\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://docs\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://spreadsheets\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://docs[0-9]\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://apirocks\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://feedproxy\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*\.youtube\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*\.ytimg\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://www\.my-debugbar\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://www\.debugbar\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://seesmic\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://dev\.chromium\.org/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://.*\.ordd\.org/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://spreadsheets[0-9]\.google\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://www\.python\.org/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://.*facebook\.com/.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://.*vimeo\.com.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://.*img\.ly.*')) return 'SOCKS5 127.0.0.1:7070';
	if (regExpMatch(url, '.*://.*\.twitgoo\.com.*')) return 'SOCKS5 127.0.0.1:7070';
	return 'DIRECT';
}
