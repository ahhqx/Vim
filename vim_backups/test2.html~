<script type="text/javascript">

//new AP.pk.pa.tempLayer();
var mytip = new AP.widget.tipdone();


//feedback's xbox
new AP.widget.xBox({
	el:D.query('.J-feedback'),
	type:'iframe',
	value:function(el){
		return '/feedbackXbox.htm';
	},
	modal:true,
	fixed: false,
