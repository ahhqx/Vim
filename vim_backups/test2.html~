<script type="text/javascript">

E.onDOMReady(function(){
	var ct = parseInt(YAHOO.util.Cookie.get("cardtype"));
    var radios=D.query('[name=cardType]');
    if(radios[ct]){
    		radios[ct].checked = 'checked';
	}
	/* 展开更多填写面板 */
	E.on('expand-all','click',function(ev){
		if(D.hasClass('expand-all-extend','fn-hide')){
			D.removeClass('expand-all-extend','fn-hide');
			D.get('expand-all').innerHTML = '取消填写更全信息';
			D.get('expand-all-value').value = 'open';
			D.addClass('J_reverse', 'J_paper');
			window.Tracker && Tracker.click('expand-all>open');	
		
		}else{
			D.addClass('expand-all-extend','fn-hide');
			D.get('expand-all').innerHTML = '若您想开店或大额收付款，请填写更全的认证信息。';
			D.removeClass('J_reverse', 'J_paper');
			D.get('expand-all-value').value = '';
			window.Tracker && Tracker.click('expand-all>close');
			D.removeClass(D.query('#expand-all-extend .fm-item'), 'fm-error');	
		}
		E.preventDefault(ev);
	});
	
	
    /* 长期选中 */
    D.get("validaty").disabled = false;
    	
	/* 有效期切换 */
	var fnLongTerm = function(){
		var _vy = D.get("validaty");
		if(D.get("long_term").checked){
			if(D.hasClass(_vy.parentNode,"fm-error")){
				D.removeClass(_vy.parentNode,"fm-error");
			}
			_vy.value = "";
			_vy.disabled = true;
			D.get("validatyFlag").value = "长期";
			D.addClass(D.query('.fm-explain', _vy.parentNode), 'fn-hide');
			
			AP.pk.cf.validator.options.rules.validaty.required = false; 
		} else {
			D.get("validaty").disabled = false;
			D.removeClass(D.query('.fm-explain', _vy.parentNode), 'fn-hide');
			D.get("validatyFlag").value = "";
			AP.pk.cf.validator.options.rules.validaty.required = true; 
		}
	}
	fnLongTerm();
	E.on('long_term','click',fnLongTerm);

	/* 身份证类型选择，有效期图示 */
	function cardCheck() {
	 	for(var j=0,radios=D.query('[name=cardType]');j<radios.length;j++){
	 		var explain = D.query('.fm-explain',D.get('J_verso'))[0], flag;
			if(radios[j].checked){
				flag = parseInt(radios[j].value);
				YAHOO.util.Cookie.set("cardtype", j);
			}
			if(flag == 0) {
				D.removeClass(['J_verso', 'J_new'], 'fn-hide');
				D.addClass(['J_old', 'J_temper'], 'fn-hide');
				D.addClass('J_verso', 'J_extend_required');
				D.get("flash-box1").setAttribute("src","http://img.alipay.net/pa/page/certify/short.gif");
				D.setStyles('flash-box1', {'top':'108px','left':'120px'}); 
			}else if(flag == 1) {
				D.addClass(['J_verso', 'J_new', 'J_temper'], 'fn-hide');
				D.removeClass('J_old', 'fn-hide');
				D.removeClass('J_verso', 'J_extend_required');
				D.removeClass('J_verso', 'fm-error');
				explain.innerHTML = '';
				D.get("flash-box1").setAttribute("src","http://img.alipay.net/pa/page/certify/kuang.gif");
				D.setStyles('flash-box1', {'top':'90px','left':'65px'});  
			}else if(flag == 2){
				D.addClass(['J_verso', 'J_new', 'J_old'], 'fn-hide');
				D.removeClass('J_temper', 'fn-hide');
				D.removeClass('J_verso', 'J_extend_required');
				D.removeClass('J_verso', 'fm-error');
				explain.innerHTML = '';
				D.get("flash-box1").setAttribute("src","http://img.alipay.net/pa/page/certify/short.gif");
				D.setStyles('flash-box1', {'top':'82px','left':'78px'});   
			}
		}
	 }
	 
	 cardCheck();
	 E.on(D.query('[name=cardType]'), 'click', cardCheck);
	 E.on("validaty", "focus", function(){
		D.removeClass("flash-box1", "fn-hide");
		D.removeClass("side-preview", "fn-hide");
	 });
	 E.on("validaty", "blur", function(){
		D.addClass("flash-box1", "fn-hide");
		D.addClass("side-preview", "fn-hide");
	 });


});	


	

</script>


