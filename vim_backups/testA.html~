<script>
var INSHELP = {
	"sysName" : "personal",
	"p01" : {
		"title0" : "001标题1",
		"content0" : "001内容[url=http://help.alipay.com/lab/210440-210464/0-210464.htm]内容[/url]内容内ddddddddddddd内容[url=http://help.alipay.com/lab/210440-210464/0-210464.htm]内容[/url]内容内容[url=http://help.alipay.com/lab/210440-210464/0-210464.htm]内容[/url]内内容[url=http://help.alipay.com/lab/210440-210464/0-210464.htm]内容[/url]内1",
		"title1" : "001标题2",
		"content1" : "001内容内容内容001内容内容内容内001内容内容内容内001内容内容内容内001内容内容内容内001内容内容内容内内容2",
		"title2" : "001标题3",
		"content2" : "001内容内[url=http://www.163.com]容内[/url]容内容3",
		"relateHelp" : "http://help.alipay.com/lab/210440-210464/0-210464.htm",
		"onlineChat" : "true",
		"hotLine" : "false"
	},
	"c02" : {
		"title0" : "002题2",
		"content0" : "002内容内容内容内容2",
		"relateHelp" : "http://www.alipay.com",
		"onlineChat" : "true",
		"hotLine" : "true"
	},
	"p03" : {
		"title0" : "003题1",
		"content0" : "003内容[url=http://www.163.com]容内[/url]内容内容内容1",
		"title1" : "",
		"content1" : "003内容内容内容内容内容内容内容2内容内容内容内容2内容内容内容内容2内容内容内容内容2内容内容内容内容2内容内容内容内容2内容内容内容内容2内容内容内容内容2内容内容内容内容2内容内容内容内容2内容2"
	}
}

var helpDom = "<div class=\"helpHead\"></div>\r\n\
<div class=\"helpBody\">\r\n\
<em class=\"m-help\">帮助信息</em>\r\n\
<a class=\"closeHelp\" href=\"javascript:void(0)\" seed=\"ERR_personal_U\">关闭</a>\r\n\
<ul class=\"helpUl\">\r\n\
</ul>\r\n\
</div>\r\n\
<div class=\"helpFoot\"></div>\r\n\
<div class=\"helpArrow\" style=\"top:20px;\"></div>"; 

var helpRankDom = "<p>是否已解决了您的问题？</p><a href=\"javascript:void(0)\" class=\"helpYes\">是</a><a href=\"javascript:void(0)\" class=\"helpNo\">否</a>";

//fm-error中含有data-errcode中插入问号,浮出层,并显示自动显示的第一个

var fmError = D.query('.fm-error');

for(var i = 0 ; i < fmError.length; i++){
	if(fmError[i].getAttribute('data-errcode').length > 0){
		var fmExplain = D.query('.fm-explain',fmError[i]),
			helpWrap = {},
			help,
			instSupIcon = {},
			errCode = fmError[i].getAttribute('data-errcode'),
			sysName = INSHELP['sysName'];//the system name.ex:personal

		D.setStyles(fmError[i],{'position':'relative'});
		D.setStyles(
			instSupIcon[i] = Element.create('span',{className:'instSupIco m-tips',appendTo:fmExplain[0]}),
			{'cursor':'pointer'});
		D.setStyles(
			helpWrap[i] = Element.create('div',{innerHTML:helpDom,className:'instanceHelp fn-hide',appendTo:fmError[i]}),
			{'left':'370px','top':'-18px','zIndex':'100'});
		instSupIcon[i].setAttribute('seed','EER' + '_' + sysName + '_'  + errCode + '_' + 'open');
		if(D.hasClass(fmError[i],'autoOpen')){
			var openFlag;//标记第一个自动显示已显示
			if(!openFlag){
				D.removeClass(helpWrap[i],'fn-hide');
				instSupIcon[i].setAttribute('seed','');
			}
			openFlag = true;
			}
				
		insertCnt(fmError[i],errCode,sysName);
	}
};

//插入帮助内容

function insertCnt(item,code,sysname){
	var helpUl = D.query('.helpUl',item),
		t = INSHELP[code],//the content of each err
		c = code,//the err code
		helpLinkDom = '',
		helpRank,
		regTitle = /^title/,
		regCnt = /^content/,
		regLink = /(\[url\=)([a-zA-Z0-9\:\/\.\?\#\-\@\&]*)(\])([\u4e00-\u9fa5a-zA-Z0-9]*)(\[\/url\])/g;
	for(var i in t){
		if(regTitle.test(i)){
			Element.create('li',{innerHTML : t[i],appendTo:helpUl[0]});
		}else if(regCnt.test(i)){
			if(regLink.test(t[i])){
				t[i] = t[i].replace(regLink,'<a href="$2" target="_blank">$4</a>');
			};
			Element.create('p',{innerHTML : t[i],appendTo:helpUl[0]});
		}else if(i == 'relateHelp'){
			if(t[i].length>0){
				helpLinkDom += '<a href="' + t[i] + '" target="_blank">更多相关帮助</a>';
			}
		}else if(i == 'onlineChat'){
			if(t[i] == 'true'){
				helpLinkDom += '<em class=\"ft-bar\">|</em><a>咨询在线客服</a>';
			}
		}else if(i == 'hotLine'){
			if(t[i] == 'true'){
				helpLinkDom += '<em class=\"ft-bar\">|</em><span>24小时服务热线：0571-88156688</span>';
			}
		}
	}
	if(helpLinkDom.length > 0){
		Element.create('div',{className:'helpLink link',innerHTML:helpLinkDom,appendTo:helpUl[0]});
	}
	helpRank = Element.create('div',{className:'helpRank fn-clear',innerHTML:helpRankDom,appendTo:helpUl[0]});

	helpRankBind(helpUl[0],helpRank,c,sysname);//add sys name to 
}

function helpRankBind(pa,current,errcode,sysname){
	var item = [D.query('.helpYes',pa)[0],D.query('.helpNo',pa)[0]];
	item[0].setAttribute('seed','ERR' + '_' + sysname + '_' + errcode + '_' + 'Y');
	item[1].setAttribute('seed','ERR' + '_' + sysname + '_' + errcode + '_' + 'N');
	E.on(item,'click',function(){
		D.addClass(current,'fn-hide');
		D.setStyles(
			Element.create('p',{className:'userRanked',innerHTML:'您的评价已提交，谢谢！',appendTo:pa}),
			{'marginTop':'10px','marginBottom':'0px','lineHeight':'20px'});
		var closeHelp = D.query('.closeHelp',pa.parentNode)[0];
		closeHelp.setAttribute('seed','');
	});
}

//绑定点击事件

var why = D.query('.instSupIco'); 
	
E.on(why,'click',function(){
	D.addClass(D.query('.instanceHelp'),'fn-hide');
	D.removeClass(D.query('.instanceHelp',this.parentNode.parentNode),'fn-hide');
	this.setAttribute('seed','');
	});

var closeHelp = D.query('.closeHelp');
	E.on(closeHelp,'click',function(){
		D.addClass(this.parentNode.parentNode,'fn-hide');	
});

</script>
